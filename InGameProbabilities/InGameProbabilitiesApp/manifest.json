{
  "manifest_version":"1",
  "type":"WebApp",
  "meta":{
    "name":"In Game Probability",
    "version":"1.0.0.0",
    "minimum-overwolf-version": "0.98.0.0",
    "author":"hackathonz",
    "icon":"icon.png",
    "icon_gray":"icon_gray.png",
    "description":"A plugin for predicting the chances of a team winning a game of league in spectator mode"
  },
  "permissions": ["Extensions", "GameInfo"],
  "data": {
    "start_window":"index",	
    "windows":{
      "index":{
        "file":"index.html",
        "transparent":true,
        "resizable":false,
        "show_in_taskbar": false,
        "clickthrough": true,
        "in_game_only": true,
        "size": {
          "width": 900,
          "height": 400
        },
        "start_position": {
          "Top": 65,
          "Left": 65
        }
      }
    },
    "game_events": [5426],
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [
            5426
          ]
        },
        "start_minimized": false
      }
    ],
	"extra-objects": {
      "InGameProbability": {
        "file": "InGameProbabilitiesPlugin.dll",
        "class": "InGameProbabilitiesPlugin.EntryPoint"
      }
    }
  }
}